+    if (_hasChanges) {
+      return await showDialog(
+        context: context,
+        builder: (context) => AlertDialog(
+          title: Text('변경 사항 취소'),
+          content: Text('프로필 변경을 취소하시겠습니까? 저장하지 않은 정보는 잃게 됩니다.'),
